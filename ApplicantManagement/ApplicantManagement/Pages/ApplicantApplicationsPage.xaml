<Page x:Class="ApplicantManagement.Pages.ApplicantApplicationsPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ApplicantManagement.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ApplicantApplicationsPage">

    <Grid>
        <DataGrid x:Name="DGApplications" IsReadOnly="True" AutoGenerateColumns="False" ColumnWidth="*" >
            <DataGrid.Columns>
                <DataGridTextColumn Header="Специальность" Binding="{Binding Speciality.SpecialityFullName}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
